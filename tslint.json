{
  "extends": [
    "tslint-microsoft-contrib"
  ],
  "rules": {
    "no-console": false,
    "prefer-const": false,
    "typedef": false,
    "no-any": false,
    "object-literal-key-quotes": false,
    "prefer-template": [
      true,
      "allow-single-concat"
    ],
    "newline-before-return": false,
    "missing-jsdoc": false,
    "import-name": false,
    "export-name": false,
    "no-unnecessary-callback-wrapper": false,
    "member-access": false,
    "no-parameter-properties": false,
    "no-relative-imports": false
  }
}
